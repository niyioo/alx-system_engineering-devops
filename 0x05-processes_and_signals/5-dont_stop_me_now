#!/usr/bin/env bash
# Bash script to stop the 4-to_infinity_and_beyond process

# Get the PID of the 4-to_infinity_and_beyond process
pid=$(pgrep -f "4-to_infinity_and_beyond")

# Check if the process is running
if [ -n "$pid" ]; then
    # Send the SIGTERM signal to the process to stop it gracefully
    kill "$pid"
    echo "Terminated"
else
    echo "The 4-to_infinity_and_beyond process is not running."
fi
